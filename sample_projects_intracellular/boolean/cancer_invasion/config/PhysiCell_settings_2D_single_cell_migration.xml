<PhysiCell_settings version="devel-version">

    <cell_rules>
        <rulesets>
            <ruleset protocol="CBHG" version="2.0" format="csv" enabled="true">
                <folder>config</folder>
                <filename>rules.csv</filename>
            </ruleset>
        </rulesets>
    </cell_rules>

    <domain>
        <x_min>-500</x_min>
        <x_max>500</x_max>
        <y_min>-500</y_min>
        <y_max>500</y_max>
        <z_min>-10</z_min>
        <z_max>10</z_max>
        <dx>20</dx>
        <dy>20</dy>
        <dz>20</dz>
        <use_2D>true</use_2D>
    </domain>

    <overall>
        <max_time units="min">2880.0</max_time>
        <time_units>min</time_units>
        <space_units>micron</space_units>
        <dt_diffusion units="min">0.01</dt_diffusion>
        <dt_mechanics units="min">0.1</dt_mechanics>
        <dt_phenotype units="min">6</dt_phenotype>
    </overall>

    <parallel>
        <omp_num_threads>10</omp_num_threads>
    </parallel>

    <save>
        <folder>output</folder>
        <full_data>
            <interval units="min">10</interval>
            <enable>true</enable>
        </full_data>
        <SVG>
            <interval units="min">10</interval>
            <enable>true</enable>
            <legend enabled="true">
                <cell_phase>false</cell_phase>
                <cell_type>true</cell_type>
            </legend>
            <plot_substrate enabled="true" limits="true">
                <substrate>ecm</substrate>
                <min_conc>0.0</min_conc>
                <max_conc>1.0</max_conc>
            </plot_substrate>
        </SVG>
        <legacy_data>
            <enable>false</enable>
        </legacy_data>
    </save>

    <options>
        <legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
        <virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>
        <disable_automated_spring_adhesions>false</disable_automated_spring_adhesions>
    </options>

    <microenvironment_setup>
        <variable name="oxygen" units="dimensionless" ID="0">
            <physical_parameter_set>
                <diffusion_coefficient units="micron^2/min">60000.0</diffusion_coefficient>
                <decay_rate units="1/min">0.0005</decay_rate>
            </physical_parameter_set>
            <initial_condition units="mmHg">38.0</initial_condition>
            <Dirichlet_boundary_condition units="mmHg" enabled="False">38.0</Dirichlet_boundary_condition>
            <Dirichlet_options>
                <boundary_value ID="xmin" enabled="False">38.0</boundary_value>
                <boundary_value ID="xmax" enabled="False">38.0</boundary_value>
                <boundary_value ID="ymin" enabled="False">38.0</boundary_value>
                <boundary_value ID="ymax" enabled="False">38.0</boundary_value>
                <boundary_value ID="zmin" enabled="False">38.0</boundary_value>
                <boundary_value ID="zmax" enabled="False">38.0</boundary_value>
            </Dirichlet_options>
        </variable>
        <variable name="ecm" units="dimensionless" ID="1">
            <physical_parameter_set>
                <diffusion_coefficient units="micron^2/min">0.0</diffusion_coefficient>
                <decay_rate units="1/min">0.0</decay_rate>
            </physical_parameter_set>
            <initial_condition units="mmHg">0.5</initial_condition>
            <Dirichlet_boundary_condition units="mmHg" enabled="False">0.0</Dirichlet_boundary_condition>
            <Dirichlet_options>
                <boundary_value ID="xmin" enabled="False">0.0</boundary_value>
                <boundary_value ID="xmax" enabled="False">0.0</boundary_value>
                <boundary_value ID="ymin" enabled="False">0.0</boundary_value>
                <boundary_value ID="ymax" enabled="False">0.0</boundary_value>
                <boundary_value ID="zmin" enabled="False">0.0</boundary_value>
                <boundary_value ID="zmax" enabled="False">0.0</boundary_value>
            </Dirichlet_options>
        </variable>
        <variable name="tgfbeta" units="dimensionless" ID="2">
            <physical_parameter_set>
                <diffusion_coefficient units="micron^2/min">0.0</diffusion_coefficient>
                <decay_rate units="1/min">0.0</decay_rate>
            </physical_parameter_set>
            <initial_condition units="mmHg">0.5</initial_condition>
            <Dirichlet_boundary_condition units="mmHg" enabled="False">0.0</Dirichlet_boundary_condition>
            <Dirichlet_options>
                <boundary_value ID="xmin" enabled="False">0.0</boundary_value>
                <boundary_value ID="xmax" enabled="False">0.0</boundary_value>
                <boundary_value ID="ymin" enabled="False">0.0</boundary_value>
                <boundary_value ID="ymax" enabled="False">0.0</boundary_value>
                <boundary_value ID="zmin" enabled="False">0.0</boundary_value>
                <boundary_value ID="zmax" enabled="False">0.0</boundary_value>
            </Dirichlet_options>
        </variable>
        <variable name="light" units="dimensionless" ID="3">
            <physical_parameter_set>
                <diffusion_coefficient units="micron^2/min">0.0</diffusion_coefficient>
                <decay_rate units="1/min">0.0</decay_rate>
            </physical_parameter_set>
            <initial_condition units="mmHg">0.0</initial_condition>
            <Dirichlet_boundary_condition units="mmHg" enabled="False">0.0</Dirichlet_boundary_condition>
            <Dirichlet_options>
                <boundary_value ID="xmin" enabled="False">0.0</boundary_value>
                <boundary_value ID="xmax" enabled="False">0.0</boundary_value>
                <boundary_value ID="ymin" enabled="False">0.0</boundary_value>
                <boundary_value ID="ymax" enabled="False">0.0</boundary_value>
                <boundary_value ID="zmin" enabled="False">0.0</boundary_value>
                <boundary_value ID="zmax" enabled="False">0.0</boundary_value>
            </Dirichlet_options>
        </variable>
        <options>
            <calculate_gradients>true</calculate_gradients>
            <track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
            <initial_condition enabled="false" type="matlab">
                <filename>./config/initial.mat</filename>
            </initial_condition>
            <dirichlet_nodes enabled="false" type="matlab">
                <filename>./config/dirichlet.mat</filename>
            </dirichlet_nodes>
        </options>
    </microenvironment_setup>

