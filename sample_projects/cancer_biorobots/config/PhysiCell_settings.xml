                <max_attachment_distance conserved="false" units="" description="">0.0</max_attachment_distance>
                <min_attachment_distance conserved="false" units="" description="">0.0</min_attachment_distance>
                <motility_shutdown_detection_threshold conserved="false" units="" description="">0.0</motility_shutdown_detection_threshold>
                <attached_worker_migration_bias conserved="false" units="" description="">0.0</attached_worker_migration_bias>
                <unattached_worker_migration_bias conserved="false" units="" description="">0.0</unattached_worker_migration_bias>
            </custom_data>
            <initial_parameter_distributions enabled="false">
          </initial_parameter_distributions>
        </cell_definition>
